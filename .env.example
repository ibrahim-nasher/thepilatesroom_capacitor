# Environment Variables Template
# Copy this file to .env and fill in your Firebase credentials

# App Configuration
VITE_APP_NAME=ThePilatesRoom
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://api.pilatesroom.com/api

# Firebase Configuration (iOS)
# Get these from Firebase Console > Project Settings > iOS App
VITE_FIREBASE_IOS_API_KEY=your_ios_api_key_here
VITE_FIREBASE_IOS_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_IOS_PROJECT_ID=your-project-id
VITE_FIREBASE_IOS_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_IOS_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_IOS_APP_ID=1:your_app_id:ios:your_ios_app_id
VITE_FIREBASE_IOS_MEASUREMENT_ID=G-XXXXXXXXXX

# Firebase Configuration (Android)
# Get these from Firebase Console > Project Settings > Android App
VITE_FIREBASE_ANDROID_API_KEY=your_android_api_key_here
VITE_FIREBASE_ANDROID_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_ANDROID_PROJECT_ID=your-project-id
VITE_FIREBASE_ANDROID_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_ANDROID_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_ANDROID_APP_ID=1:your_app_id:android:your_android_app_id
VITE_FIREBASE_ANDROID_MEASUREMENT_ID=G-XXXXXXXXXX

# Firebase Configuration (Web - for browser testing only)
VITE_FIREBASE_WEB_API_KEY=your_web_api_key_here
VITE_FIREBASE_WEB_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_WEB_PROJECT_ID=your-project-id
VITE_FIREBASE_WEB_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_WEB_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_WEB_APP_ID=1:your_app_id:web:your_web_app_id
VITE_FIREBASE_WEB_MEASUREMENT_ID=G-XXXXXXXXXX

